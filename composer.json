{
    "name": "condividendo/laravel-fatturapa",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "bensampo/laravel-enum": "^6.12",
        "brick/math": "^0.14",
        "illuminate/support": "^12.31",
        "illuminate/contracts": "^12.31",
        "thecodingmachine/safe": "^3.3"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0",
        "larastan/larastan": "^3.7",
        "laravel/pint": "^1.25",
        "orchestra/testbench": "^10.6",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "rector/rector": "^2.1",
        "thecodingmachine/phpstan-safe-rule": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Condividendo\\FatturaPA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Condividendo\\FatturaPA\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "rector": "vendor/bin/rector",
        "dry-rector": "vendor/bin/rector --dry-run",
        "pint": "vendor/bin/pint",
        "dry-pint": "vendor/bin/pint --test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Condividendo\\FatturaPA\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
